Cell[BoxData[
    \(starPolygon[Polygon[{p1_, p2_, p3_, p4_}], f_] := 
      Module[{m = 1\/4\ \((p1 + p2 + p3 + p4)\), q1 = \(p4 + p1\)\/2, 
          q2 = \(p1 + p2\)\/2, q3 = \(p2 + p3\)\/2, q4 = \(p3 + p4\)\/2, r1, 
          r2, r3, r4}, {r1, r2, r3, r4} = 
          \((m + f\ \((#1 - m)\)&)\)/@{q1, q2, q3, q4}; {
          Polygon/@{{r1, p1, r2}, {r2, p2, r3}, {r3, p3, r4}, {r4, p4, r1}}, 
          Line/@{{r1, r2, r3, r4, r1}, {p1, r2, p2, r3, p3, r4, p4, r1, p1}}}]
        \)], "Input",
  CellTags->"S0.0.6"],

