Cell[TextData[{
  StyleBox[
  "For Riemann surfaces with more than two or three sheets we typically \
encounter the problem that it is hard to look 'inside'. To have a better look \
in the inner parts of the surface we define a function ", "TI"],
  StyleBox["holePolygon", "MR",
    FontFamily->"Courier",
    FontWeight->"Bold"],
  StyleBox[" which cuts a hole of 'size' f in a polygon. ", "TI"]
}], "Text",
  CellTags->{"S0.0.2", "2.15"}],

Cell[BoxData[
    \(holePolygon[Polygon[l_], f_] := 
      Module[{m = Plus@@l\/Length[l], innerPoints}, 
        innerPoints = \((m + f\ \((#1 - m)\)&)\)/@l; {
          MapThread[
            Polygon[Join[#1, Reverse[#2]]]&, {
